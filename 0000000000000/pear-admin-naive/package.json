  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build:no-check": "vite build",
    "build": "vite build && vue-tsc --noEmit",
    "preview": "vite preview",
    "prettier:fix": "yarn prettier --write .",
    "prepare": "husky install",
    "lint:script": "eslint --ext .ts,vue,tsx --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{css,less,vue,tsx}"
  },
  "dependencies": {
    "@iconify/iconify": "^2.0.3",
    "@vueuse/core": "^5.1.3",
    "animate.css": "^4.1.1",
    "lodash-es": "^4.17.21",
    "omit.js": "^2.0.2",
    "pinia": "^2.0.0-beta.5",
    "qs": "^6.10.1",
    "vue": "^3.2.0-beta.3",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@iconify/json": "^1.1.376",
    "@purge-icons/generated": "^0.7.0",
    "@types/node": "^16.3.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vicons/antd": "^0.10.0",
    "@vicons/ionicons5": "^0.10.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/compiler-sfc": "^3.2.0-beta.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.3.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.30.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "lint-staged": ">=10",
    "naive-ui": "^2.15.10",
    "postcss": "^8.3.5",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.2",
    "vfonts": "^0.1.0",
    "vite": "^2.4.0",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-svg-icons": "^1.0.1",
    "vue-tsc": "^0.0.24"
  },
  "lint-staged": {
    "*.{ts,vue,tsx}": "eslint --fix",
    "*.{css,less,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lintstage",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
