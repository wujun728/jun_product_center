      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "febs",
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wuyouzhuguli/FEBS-Cloud"
  },
  "bugs": {
    "url": "https://github.com/wuyouzhuguli/FEBS-Cloud/issues"
  },
  "dependencies": {
