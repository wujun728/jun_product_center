<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
    http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd" version="1.0">
    
	<persistence-unit name="XSpringJpa"
		transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>com.jun.plugin.common.dataaccess.entity.BaseEntity</class>
		<class>com.jun.plugin.common.dataaccess.entity.OrderEntity</class>
		<class>com.jun.plugin.common.dataaccess.entity.VersionEntity</class>
		<class>com.jun.plugin.common.tree.EasyUITreeNode</class>
		<class>com.jun.plugin.common.tree.enumpath.TreeNodeEntity</class>
	</persistence-unit>

</persistence>
